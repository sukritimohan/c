#include <stdio.h>
#include <stdlib.h>

int main()
{
   int i,j,k,m,l,a,b,c;
   for(i=1;i<=5;i++)
   {
       for(j=5;j>=i;j--)
        printf(" ");
       for(k=1;k<(2*i);k++)
       {
           if(k%2!=0)
            printf("*");
           else
            printf(" ");
       }
       printf("\n");
   }

   for(a=1;a<=5;a++)
   {
       for(b=1;b<=a;b++)
        printf(" ");
       for(c=1;c<=(10-2*a);c++)
       {
           if(c%2!=0)
            printf(" ");
           else
            printf("*");
       }
       printf("\n");
   }
}
